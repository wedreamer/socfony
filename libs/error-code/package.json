{
  "name": "@socfony/error-code",
  "version": "0.0.4",
  "description": "Socfony - Error codes",
  "keywords": [
    "socfony",
    "error"
  ],
  "author": {
    "name": "Socfony",
    "url": "https://github.com/socfony"
  },
