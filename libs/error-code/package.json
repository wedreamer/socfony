  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --build tsconfig.lib.json",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "bugs": {
    "url": "https://github.com/socfony/socfony/issues"
